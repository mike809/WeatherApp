<%= form_with(url: weather_forecasts_path, html: { class: 'form-group' },
        data: { controller: 'address', action: 'keydown.enter->address#preventSubmit' }) do |form| %>
  <div class='form-group'>
    <%= form.label :address, class: 'form-label', for: 'autocomplete' %>
    <%= form.text_field :address, id: 'autocomplete', class: 'form-control' %>
  </div>
<% end %>
